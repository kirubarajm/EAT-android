<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context="com.tovologies.tovomoveit.ui.tandc.TermsAndConditionActivity">

    <data>

        <import type="android.view.View" />


        <variable
            name="termsViewModel"
            type="com.tovo.eat.ui.signup.tandc.TermsAndConditionViewModel" />

    </data>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">


        <android.support.v7.widget.Toolbar
            android:id="@+id/toolbar1"
            style="@style/Toolbar"
            android:layout_alignParentTop="true"
            android:background="@color/primary_new"
            android:visibility="visible"
            app:subtitleTextColor="@color/white"
            app:title="@string/terms_and_condition"
            app:titleTextAppearance="@style/Toolbar.TitleText" />

        <ScrollView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_below="@+id/toolbar1">

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:padding="40dp">

                <com.tovo.eat.utilities.fonts.CircularTextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="sefefgergsergerg"
                    android:textColor="@color/dark_gray"
                    android:textSize="14sp" />

            </RelativeLayout>
        </ScrollView>

    </RelativeLayout>

</layout>